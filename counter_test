#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x563ee69b6dc0 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
v0x563ee6a03070_0 .var "iclk", 0 0;
v0x563ee6a03140_0 .var "irst", 0 0;
v0x563ee6a03210_0 .net "ocnt", 3 0, v0x563ee6a02f30_0;  1 drivers
S_0x563ee69b6f50 .scope module, "counter_test" "counter" 2 13, 3 1 0, S_0x563ee69b6dc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "irst";
    .port_info 1 /INPUT 1 "iclk";
    .port_info 2 /OUTPUT 4 "ocnt";
v0x563ee69b87b0_0 .net "iclk", 0 0, v0x563ee6a03070_0;  1 drivers
v0x563ee69b8ba0_0 .net "irst", 0 0, v0x563ee6a03140_0;  1 drivers
v0x563ee6a02f30_0 .var "ocnt", 3 0;
E_0x563ee69f2730 .event posedge, v0x563ee69b87b0_0;
    .scope S_0x563ee69b6f50;
T_0 ;
    %wait E_0x563ee69f2730;
    %load/vec4 v0x563ee69b8ba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x563ee6a02f30_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563ee6a02f30_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x563ee6a02f30_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563ee69b6dc0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563ee6a03140_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563ee6a03070_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x563ee69b6dc0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563ee6a03140_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563ee6a03140_0, 0, 1;
    %delay 300, 0;
    %vpi_call 2 8 "$stop" {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x563ee69b6dc0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x563ee6a03070_0;
    %nor/r;
    %store/vec4 v0x563ee6a03070_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x563ee69b6dc0;
T_4 ;
    %vpi_call 2 19 "$monitor", "At time %t, ocnt = %d", $time, v0x563ee6a03210_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x563ee69b6dc0;
T_5 ;
    %vpi_call 2 22 "$dumpfile", "counter_test.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563ee69b6f50 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
